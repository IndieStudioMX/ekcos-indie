.modal-body 
	padding 14px

.modal-content 
	background rgb(208,208,208) /* Old browsers */
	background -moz-linear-gradient(to bottom, rgba(208, 208, 208, 0.3) 0%, rgba(224, 224, 223, 0.84) 56%, rgba(254, 255, 255, 0.91) 100%) /* FF3.6+ */
	background -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(208,208,208,.3)), color-stop(56%,rgba(224,224,223,.84)), color-stop(100%,rgba(254,255,255,.91))) /* Chrome,Safari4+ */
	background -webkit-linear-gradient(to bottom, rgba(208, 208, 208, 0.3) 0%, rgba(224, 224, 223, 0.84) 56%, rgba(254, 255, 255, 0.91) 100%)/* Chrome10+,Safari5.1+ */
	background -o-linear-gradient(to bottom, rgba(208, 208, 208, 0.3) 0%, rgba(224, 224, 223, 0.84) 56%, rgba(254, 255, 255, 0.91) 100%) /* Opera 11.10+ */
	background -ms-linear-gradient(to bottom, rgba(208, 208, 208, 0.3) 0%, rgba(224, 224, 223, 0.84) 56%, rgba(254, 255, 255, 0.91) 100%) /* IE10+ */
	background linear-gradient(to bottom, rgba(208, 208, 208, 0.3) 0%, rgba(224, 224, 223, 0.84) 56%, rgba(254, 255, 255, 0.91) 100%)
	/*background url('../images/modalPower.png')*/
	border-radius 0
	height 84rem
	background-repeat no-repeat
	/*width 806px*/
	width 947px
	margin 0 auto
	top 2rem 
	padding-left 0
	margin-left -18rem

li.flip-item.flip-current 
	box-shadow 0 0 73px rgba(255, 255, 255, 1)

.modal-header, .modal-footer
	border-bottom 0
	
.modal-content-eckos 
	background rgb(208,208,208) /* Old browsers */
	background -moz-linear-gradient(to bottom, rgba(208, 208, 208, 0.3) 0%, rgba(224, 224, 223, 0.84) 56%, rgba(254, 255, 255, 0.91) 100%) /* FF3.6+ */
	background -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(208,208,208,.3)), color-stop(56%,rgba(224,224,223,.84)), color-stop(100%,rgba(254,255,255,.91))) /* Chrome,Safari4+ */
	background -webkit-linear-gradient(to bottom, rgba(208, 208, 208, 0.3) 0%, rgba(224, 224, 223, 0.84) 56%, rgba(254, 255, 255, 0.91) 100%)/* Chrome10+,Safari5.1+ */
	background -o-linear-gradient(to bottom, rgba(208, 208, 208, 0.3) 0%, rgba(224, 224, 223, 0.84) 56%, rgba(254, 255, 255, 0.91) 100%) /* Opera 11.10+ */
