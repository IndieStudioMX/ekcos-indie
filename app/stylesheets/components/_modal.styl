@media screen and (min-width 200px) and (max-width 600px) and (min-height 480px)
	.modal-content 
		width 100% !important
		margin-left 0 !important
		height 210rem !important
	.modal-title 
		text-align center
	.docs-img, .img-responsive
		margin 0 auto
		
	.power-m
		display inline-block 
		vertical-align middle 
		width 48%
		margin .5rem 0 
		padding 0
	
	.ek-m, .ek-it, .ez-d, .ez-img, .tru-d
		display inline-block 
		vertical-align middle 
		width 48% 
		margin .5rem 0
		padding 0
		margin .5rem auto .5rem auto
	.ez-img, .tru-img 
		width 32%
	
	.ek-content
		height 250.5rem !important 
		
	.modal-content-ez 
		height: 132rem !important;
		
	.modal-content-tru 
		height 132rem !important
		
