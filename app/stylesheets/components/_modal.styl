.it-power::after,.ic-power::after, .ig-power::after
	content ""
	display inline-block 
	position absolute 
	height 27px 
	width 40px 
	top .65rem
	right 1rem  
	background url('../images/sprite-modal.png')
	background-repeat no-repeat 
	background-position -6px -10px
	background-size 364%
	
.it-power::after
	background-position -105px -10px
	right 2rem 
	
.ig-power::after
	background-position -57px -10px
	right 30rem 
	
