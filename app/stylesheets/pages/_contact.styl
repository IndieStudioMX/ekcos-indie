@import "helpers/_mixins"

a:hover
	text-decoration none
	color black 

.contact-wrapper
	height auto
	width 100%
	position relative
	top 5rem

.contact-container
	background url('../images/power-bg.jpg') no-repeat
	background-size 100% 100%
	position relative
	with 100%
	height 289rem
	.contact-quote-container
		margin-top 15rem
		.contact-quote-title
			border-bottom 2px solid rgba(0,0,0,.3)
			margin 0 auto
			width 25%
			font-size 4rem
			margin-bottom 2rem
			text-transform uppercase

.container-c
	width 1000px
	margin 1rem auto 0 auto
	padding-top 1rem

.contact-links-title
	height 70px
	line-height 10px
	background rgb(204,204,51)
	width 88rem
	margin-left 6rem 
	h3
		font-size 2.4rem
		font-weight bold
		margin-top 0
	.flag 
		display block  
		width 110px 
		height 110px
		background url('../images/contact-sprite.png')
		background-repeat no-repeat
		background-position 1px -1px 
		background-size 1060px
		position absolute
		top -2.2rem
		left -6.42rem 

.contact-links-content_title-asia 
	margin-top 1rem

.contact-links-title-int
	height auto
	padding 1rem 0
	margin-left 2.6rem 
	width 91.2rem 
		
.contact-links-content
	margin 2rem auto 1rem auto
	height 130px
	padding 1rem 2rem
	width 80%
	background rgba(0,0,0,.2)
	line-height 18px
	font-size 1.5rem
	.contact-links-content_author, .contact-links-content_author-x,
	.contact-links-content_author-mex
		border-bottom 2px solid rgba(0,0,0,.3)
		width 42%
		font-weight bold
	.contact-links-content_author-x
		width 65%
	.contact-links-content_author-mex
		width 50%
	.contact-links-content_mail, .contact-links-content_phone 
		color black
		font-weight bold
		display block
	.info-container 
		border 2px dashed rgba(0,0,0,.3)
		height 125px
		width 111%
		margin -.7rem 0 -.7rem -1.8rem
		padding .1rem .7rem
		.contact-links-content_mail
			font-weight normal
	
.mail-margin
	margin-top -1rem

.contact-links-title_mex
	height 70px
	margin-left 7rem
	width 86.8rem

.mex 
	background-position 1px -108px !important
	background-size 1060px !important
	position absolute
	top -1.5rem !important
	left -7.4rem !important

.contact-links-header,
.contact-links-header-cam,
.contact-links-header-sk
	margin 3rem auto 1rem auto
	height 60px
	padding .1rem 3rem
	width 80%
	background rgba(0,0,0,.2)
	line-height 15px
	font-size 1.5rem

.flag-int 
	display block  
	height 90px !important
	width 90px !important
	background url('../images/contact-sprite.png')
	background-repeat no-repeat
	background-position 1px -1px 
	background-size 1060px
	position absolute
	top -2.5rem
	left -2.4rem 

.contact-links-header-cam
	width 53rem !important
	position relative
	left -23.6rem
	top 6rem

.contact-links-content_author-mex 
	margin-left 3rem

.bar 
	display block 
	height 130px !important
	width 130px !important
	background url('../images/bermudas-01.png')
